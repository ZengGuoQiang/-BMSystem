{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true,
            "arrowFunctions": true,
            "classes": true,
            "modules": true,
            "defaultParams": true
        },
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": 0,
        "no-class-assign": 2,
        "no-continue": 0,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-label": 0,
        "no-eq-null": 2,
        "no-extra-bind": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 1,
        "no-func-assign": 2,
        "no-implicit-coercion": 1,
        "no-invalid-regexp": 2,
        "no-invalid-this": 2,
        "no-irregular-whitespace": 2,
        "no-label-var": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-loop-func": 1,
        "no-multi-spaces": 1,
        "linebreak-style": [
            0,
            "windows"
        ],
        "no-mixed-spaces-and-tabs": [
            2,
            false
        ],
        "no-mixed-requires": [
            0,
            false
        ],
        "no-multi-str": 2,
        "no-multiple-empty-lines": [
            1,
            {
                "max": 2
            }
        ],
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 0,
        "no-new": 1,
        "no-plusplus": 0,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-return-assign": 1,
        "no-self-compare": 2,
        "no-sequences": 0,
        "no-shadow": 2,
        "no-spaced-func": 2,
        "no-trailing-spaces": 1,
        "no-this-before-super": 0,
        "no-undef": 1,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-unexpected-multiline": 2,
        "no-unneeded-ternary": 2,
        "no-unreachable": 2,
        "arrow-parens": 0,
        "arrow-spacing": 2,
        "callback-return": 1,
        "camelcase": 2,
        "comma-spacing": 0,
        "comma-style": [
            2,
            "last"
        ],
        "consistent-return": 1,
        "constructor-super": 0,
        "eqeqeq": 2,
        "func-names": 0,
        "guard-for-in": 0,
        "init-declarations": 0,
        "new-parens": 2,
        "newline-after-var": 2,
        "semi": [
            2,
            "always"
        ],
        "sort-vars": 0,
        "space-after-keywords": [
            0,
            "always"
        ],
        "space-before-blocks": [
            0,
            "always"
        ],
        "space-in-parens": [
            0,
            "never"
        ],
        "spaced-comment": 0,
        "use-isnan": 2,
        "wrap-regex": 0,
        "valid-typeof": 2,
        "vars-on-top": 2,
        "yoda": [
            2,
            "never"
        ],
        "no-console": 0,
        "no-const-assign": 2
    }
}